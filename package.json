  "dependencies": {
    "@types/yup": "^0.29.4",
    "@unform/core": "^2.1.3",
    "@unform/web": "^2.1.3",
    "css-loader": "^4.2.0",
    "date-fns": "^2.15.0",
    "electron-store": "^6.0.0",
    "json-schema-typed": "^7.0.3",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "polished": "^3.6.5",
    "react": "^16.13.1",
    "react-contextmenu": "^2.14.0",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-modal": "^3.11.2",
    "react-spring": "^8.0.27",
    "react-table": "^7.5.0",
    "react-tabs": "^3.1.1",
    "recoil": "^0.0.10",
    "style-loader": "^1.2.1",
    "styled-components": "^5.1.1",
    "uuidv4": "^6.2.1",
    "yup": "^0.29.2"
  },
  "build": {
    "productName": "Coddee SQL",
    "appId": "your.id",
    "files": [
      "dist/"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "identity": null
    },
    "directories": {
      "buildResources": "build",
      "output": "packages"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
